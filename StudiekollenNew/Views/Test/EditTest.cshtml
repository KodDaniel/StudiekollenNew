@model StudiekollenNew.ViewModels.TestViewModels.EditTestViewModel
@{
    ViewBag.Title = "EditTest";
}

<h2>Redigera test</h2>

<h2>@Model.TestName</h2>


<table id="customers" class="table table-bordered table-hover">

    <tr>
        <th>Fråga</th>
        <th>Svar</th>
        <th></th>
    </tr>
       
    @foreach (var item in Model.QuestionsModels)
    {
        <tr>                 
            <td>@item.Key</td>
            <td>@item.Value</td>         
            <td>@Html.ActionLink("Radera fråga", "DeleteQuestion", "Test", new {key = item.Key}, null)</td>
        </tr>
    }

</table>
